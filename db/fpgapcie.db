record(ai, "bar2:read")
{
	field(DTYP, "asynInt32")
	field(DESC, "sdram acquire")
	field(SCAN,"$(SCAN=1 second)")
	field(INP, "@asyn($(PORT),0,$(TIMEOUT)) SDRAM_ACQUIRE_SingleValue")
}
record(ao, "bar2:write")
{
	field(DTYP, "asynInt32")
	field(DESC, "sdram acquire")
	field(OUT, "@asyn($(PORT),0,$(TIMEOUT)) SDRAM_ACQUIRE_SingleValue")
	field(SCAN,"Passive")
}
