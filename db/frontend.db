record(mbbi, "BPM:FRONTEND$(PORT):channel1:switchState:read"){
	field(DTYP, "asynInt32")
	field(DESC, "Switch state")
	field(INP, "@asyn($(PORT),0,$(TIMEOUT))S_State")
	field(SCAN,"$(SCAN=1 second)")
	field(NOBT,"2")
	field(ZRVL,"0")
	field(ONVL,"1")
	field(TWVL,"2")
	field(THVL,"3")
	field(ZRST,"DIRECT")
	field(ONST,"INVERT")
	field(TWST,"MATCHED")
	field(THST,"SWITCHING")
}
