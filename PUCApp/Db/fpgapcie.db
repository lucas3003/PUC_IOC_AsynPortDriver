record(waveform, "BPM:FPGA:dataAcquisition:waveform")
{
    field(PINI, "NO")
    field(DTYP, "asynFloat32ArrayIn")
    field(INP,  "@asyn($(PORT),0,5)Waveform")
    field(FTVL, "FLOAT")
    field(NELM, "6280")
    field(SCAN, "1 second")
    field(PINI, "NO")
}
