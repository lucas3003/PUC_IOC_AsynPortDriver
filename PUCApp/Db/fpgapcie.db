record(ai, "bar0:read")
{
	field(DTYP, "asynInt32")
	field(DESC, "bar0")
	field(SCAN,"$(SCAN=1 second)")
	field(INP, "@asyn($(PORT),0,$(TIMEOUT))bar0")
	field(PREC, "3")
	field(EGU, "Celsius")
	field(EGUF, "100.0")
	field(EGUL, "0.0")
	field(LINR, "LINEAR")
}
record(ao, "bar0:write")
{
	field(DTYP, "asynInt32")
	field(DESC, "bar0")
	field(OUT, "@asyn($(PORT),0,$(TIMEOUT))bar0")
	field(PREC, "3")
	field(SCAN,"Passive")
	field(EGU, "Celsius")
	field(LINR, "NO CONVERSION")
	field(PINI, "YES")
}
