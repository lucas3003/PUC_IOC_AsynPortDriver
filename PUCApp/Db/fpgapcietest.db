record(stringout, "BPM:FPGA:dataAcquisition:directory")
{
	field(DTYP, "asynOctetWrite")
	field(DESC, "Data acquisition directory")
	field(OUT, "@asyn($(PORT),0,$(TIMEOUT))DAdirectory")
	field(PINI, "YES")
}
record(waveform, "BPM:FPGA:dataAcquisition:waveform")
{
    field(DTYP, "asynFloat32ArrayIn")
    field(INP,  "@asyn($(PORT),0,$(TIMEOUT))Waveform")
    field(FTVL, "FLOAT")
    field(NELM, "50")
    field(SCAN,"1 second")
}
record(mbbi, "dbtest"){
	field(DTYP, "asynInt32")
	field(DESC, "test")
	field(INP, "@asyn($(PORT),0,$(TIMEOUT))dbtest")
	field(SCAN,"1 second")
	field(NOBT,"2")
	field(ZRVL,"0")
	field(ONVL,"1")
	field(TWVL,"2")
	field(THVL,"3")
	field(ZRST,"0")
	field(ONST,"1")
	field(TWST,"2")
	field(THST,"3")
}
